{
  "name": "book_a_meal",
  "version": "1.0.0",
  "description": "For andela stuff",
  "main": "index.js",
  "scripts": {
    "clean-start": "node build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-server",
    "test": "mocha --require @babel/register ./test/*.js  --timeout 100000 --exit || true",
    "test-default": "nyc mocha --exit --timeout 100000 --require @babel/register ./test/*.js",
    "start": "nodemon --exec babel-node server/index.js",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preciousomonze/alc-book-meal.git"
  },
  "author": "preciousomonze (CODE EXPLORER)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/preciousomonze/alc-book-meal/issues"
  },
  "homepage": "https://github.com/preciousomonze/alc-book-meal#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "esm": "^3.2.6",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "pg": "^7.8.0",
    "sequelize": "^4.42.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/helpers": "^7.3.1",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "should": "^13.2.3"
  }
}
